{
  "name": "line-login-test-app",
  "version": "1.0.0",
  "description": "This is sample app to experience LINE login",
  "main": "index.js",
  "scripts": {
    "client": "webpack-dev-server --config ./webpack.dev.js",
    "server": "NODE_ENV=development nodemon src/server/index.js",
    "build": "webpack --config ./webpack.prod.js",
    "dev": "run-p client server",
    "serve": "firebase emulators:start",
    "start": "run-s build serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --fix 'src/**/*.js'",
    "format": "prettier --write src/**/*.js",
    "lint-fix": "run-s lint format",
    "prepare": "husky install",
    "shell": "firebase functions:shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "author": "shun-harutaro",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "babel-loader": "^9.1.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.1",
    "firebase-functions-test": "^3.1.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "@line/liff": "^2.22.2",
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase-admin": "^11.10.1",
    "firebase-functions": "^4.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0"
  }
}
